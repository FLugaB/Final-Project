{
  "name": "server",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "nodemon bin/www",
    "test:up": "npx sequelize-cli db:drop --env=test && npx sequelize-cli db:create --env=test && npx sequelize-cli db:migrate --env=test && npx sequelize-cli db:seed:all --env test",
    "test:down": "npx sequelize-cli db:migrate:undo:all --env=test",
    "test": "clear && jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test1": "clear && jest --coverage -i productController.test.js --runInBand --detectOpenHandles --forceExit",
    "test2": "clear && jest -i clientController.test.js --coverage --runInBand --detectOpenHandles --forceExit",
    "test3": "clear && jest -i cmsController.test.js --coverage --runInBand --detectOpenHandles --forceExit",
    "test4": "clear && jest -i doctorController.test.js --coverage --runInBand --detectOpenHandles --forceExit",
    "dev": "nodemon app.js",
    "dev:up": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "dev:down": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "form-data": "^4.0.0",
    "imagekit": "^3.2.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "node-fetch": "^3.2.0",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "sequelize": "^6.13.0"
  },
  "devDependencies": {
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.2"
  },
  "bin": {
    "server": "www.js"
  },
  "description": ""
}